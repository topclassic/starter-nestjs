{
  "name": "starter-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "CHOTIPAT POOWONGTHANARAT",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "nodemon",
    "debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/graphql": "^6.4.3",
    "@nestjs/platform-express": "^6.8.3",
    "@nestjs/platform-fastify": "^6.6.1",
    "@nestjs/typeorm": "^6.2.0",
    "@types/lodash": "^4.14.149",
    "apollo-server-fastify": "^2.9.1",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "cache-manager": "^2.10.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "dotenv": "^8.1.0",
    "graphql": "^14.5.4",
    "graphql-tools": "^4.0.5",
    "include-all": "^4.0.3",
    "jsonwebtoken": "^8.5.1",
    "lodash-uuid": "^0.0.3",
    "mongoose": "^5.6.11",
    "pg": "^7.12.1",
    "ramda": "^0.26.1",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "require-all": "^3.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "sequelize": "^5.21.3",
    "sequelize-typescript": "^1.1.0",
    "short-id": "^0.1.0-1",
    "typeorm": "^0.2.20",
    "typescript": "^3.7.4",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "2.0.7",
    "jest": "24.7.1",
    "nodemon": "^1.14.1",
    "prettier": "1.17.0",
    "sequelize-cli": "^5.5.1",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
